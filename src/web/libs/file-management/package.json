  },
  "devDependencies": {
    "@sisa/eslint-config": "workspace:*",
    "@sisa/tsconfig": "workspace:*",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "eslint": "^8.52.0",
    "react": "^18.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/joy": "^5.0.0-beta.10",
    "lucide-react": "^0.288.0",
