{
  "name": "@sisa/file-management",
  "version": "0.0.0",
  "main": "./src/index.ts",
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
